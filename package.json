{
  "name": "@rebeldotcom/components",
  "version": "0.4.3",
  "description": "Rebel web components",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "version:patch": "npm version patch && git push",
    "version:minor": "npm version minor && git push",
    "deploy:patch": "yarn build && yarn version:patch && npm publish",
    "deploy:minor": "yarn build && yarn version:minor && npm publish",
    "clean": "rimraf node_modules && npm install",
    "eslint": "eslint src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rebeldotcom/rebel-web-components.git"
  },
  "author": "",
  "license": "ISC",
